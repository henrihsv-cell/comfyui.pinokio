{
  "daemon": true,
  "run": [{
    "method": "shell.run",
    "params": {
      "venv": "env",
      "path": "ComfyUI",
      "message": "python main.py --cpu",
      "on": [{
        "event": "/(http:\\/\\/[0-9.:]+)/",
        "done": true
      }, {
        "event": "/errno/i",
        "break": false
      }, {
        "event": "/error:/i",
        "break": false
      }]
    }
  }, {
    "method": "local.set",
    "params": {
      "url": "{{input.event[1]}}"
    }
  }]
}
